Bootstrap: localimage
From: /home/aaron.m.janssen/Desktop/WebODMRewrite/WebODMCompat/base.sif

%files
    . /webodm

%post
    cd /webodm
    npm install --quiet && webpack --mode production && \
    echo "UTC" > /etc/timezone && \
    python manage.py collectstatic --noinput && \
    python manage.py rebuildplugins && \
    python manage.py translate build --safe && rm /webodm/webodm/secret_key.py

